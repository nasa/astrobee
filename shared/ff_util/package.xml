<?xml version="1.0"?>
<package format="3">
  <name>ff_util</name>
  <version>1.0.0</version>
  <description>
    Package for ff nodelet stuff
  </description>
  <license>
    Apache License, Version 2.0
  </license>
  <author email="astrobee-fsw@nx.arc.nasa.gov">
    Astrobee Flight Software
  </author>
  <maintainer email="astrobee-fsw@nx.arc.nasa.gov">
    Astrobee Flight Software
  </maintainer>
  <build_depend condition="$ROS_VERSION == 1">catkin</build_depend>
  <build_depend condition="$ROS_VERSION == 2">ament_cmake</build_depend>
  
  <build_depend condition="$ROS_VERSION == 1">roscpp</build_depend>
  <build_depend condition="$ROS_VERSION == 2">rclcpp</build_depend>

  <build_depend>roslib</build_depend>
  <build_depend>nodelet</build_depend>
  <build_depend>pluginlib</build_depend>
  <build_depend>actionlib</build_depend>
  <build_depend>diagnostic_msgs</build_depend>
  <build_depend>dynamic_reconfigure</build_depend>
  <build_depend>ff_msgs</build_depend>
  <build_depend>tf2_geometry_msgs</build_depend>
  <build_depend>config_reader</build_depend>
  <build_depend>msg_conversions</build_depend>

  <exec_depend condition="$ROS_VERSION == 1">roscpp</exec_depend>
  <exec_depend condition="$ROS_VERSION == 2">rclcpp</exec_depend>
  <exec_depend>roslib</exec_depend>
  <exec_depend condition="$ROS_VERSION == 1">nodelet</exec_depend>
  <exec_depend>pluginlib</exec_depend>
  <exec_depend>actionlib</exec_depend>
  <exec_depend>diagnostic_msgs</exec_depend>
  <exec_depend>dynamic_reconfigure</exec_depend>
  <exec_depend>ff_msgs</exec_depend>
  <exec_depend>tf2_geometry_msgs</exec_depend>
  <exec_depend>config_reader</exec_depend>
  <exec_depend>msg_conversions</exec_depend>
  <export>
    <build_type condition="$ROS_VERSION == 1">catkin</build_type>
    <build_type condition="$ROS_VERSION == 2">ament_cmake</build_type>
  </export>
</package>
